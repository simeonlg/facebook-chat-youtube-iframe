{
	"manifest_version": 2,
	"name": "Facebook chat youtube iFrame",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"https://www.messenger.com/*"
			],
			"js": ["jquery-2.2.0.min.js", "content.js"],
			"css": ["style.css"],
			"run_at": "document_end",
			"web_accessible_resources": ["youtube.png"]
		}
	]
}